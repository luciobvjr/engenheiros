{
  "builds": [
    {
      "src": "connections/backend/src/index.ts",
      "use": "@vercel/node",
      "config": {
        "includeFiles": ["connections/backend/dist/**/*"]
      }
    }
  ],  
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/connections/backend/dist/index.js"
    }
  ]
}